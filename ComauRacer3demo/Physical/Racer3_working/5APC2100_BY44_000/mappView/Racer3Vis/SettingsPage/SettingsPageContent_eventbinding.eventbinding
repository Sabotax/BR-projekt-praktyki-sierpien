        <Sequence>
          <Step order="0">
            <Action>
              <Target xsi:type="clientSystem.Action">
                <Method xsi:type="clientSystem.Action.SetMeasurementSystem" value="metric" />
              </Target>
            </Action>
          </Step>
          <Step order="1">
            <Action>
