        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.ChangeTheme" theme="BuRThemeFlat2" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="SettingsPageContent.btnLeft3SettingsContentSetLanguageEn.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="SettingsPageContent" widgetRefId="btnLeft3SettingsContentSetLanguageEn" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
