          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="DocsBrowserPageContent.btnEnterFolder.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="DocsBrowserPageContent" widgetRefId="btnEnterFolder" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="opcUa.NodeAction" refId="::FileManage:cmd.MpFileManagerUIConnectType.File.EnterFolder">
