    <EventBinding id="DocsBrowserPageContent.btnDelete.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="DocsBrowserPageContent" widgetRefId="btnDelete" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="opcUa.NodeAction" refId="::FileManage:cmd.MpFileManagerUIConnectType.File.Delete">
            <Method xsi:type="opcUa.NodeAction.SetValueBool" value="true" />
