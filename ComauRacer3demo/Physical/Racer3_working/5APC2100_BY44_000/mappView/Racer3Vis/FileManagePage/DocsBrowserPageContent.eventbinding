        </Action>
      </EventHandler>
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.OpenDialog" dialogId="DialogConfirm" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
